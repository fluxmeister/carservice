package jwd.stanica;

import javax.annotation.PostConstruct;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import jwd.stanica.model.Prevoznik;
import jwd.stanica.service.PrevoznikService;

@Component
public class TestData {
	
	@Autowired
	private PrevoznikService ps;
	
	@PostConstruct
	public void init() {
		Prevoznik p = new Prevoznik();
		p.setNaziv("Lasta");
		p.setPib("12332111");
		p.setAdresa("Beogradski put 11, Beograd");
		
		ps.save(p);
		
		Prevoznik p2 = new Prevoznik();
		p2.setNaziv("Ekspres");
		p2.setPib("12332112");
		p2.setAdresa("Bajski put 1, Subotica");
		
		ps.save(p2);
		
		Prevoznik p3 = new Prevoznik();
		p3.setNaziv("Barakuda");
		p3.setPib("12458795");
		p3.setAdresa("Bulervar Revolucije 135/42, Niš");
		
		ps.save(p3);
		
		Prevoznik p4 = new Prevoznik();
		p4.setNaziv("GibraltarTours");
		p4.setPib("25489564");
		p4.setAdresa("Sipovačka 89, Knjaževac");
		
		ps.save(p4);
		
		
	}
	
}
